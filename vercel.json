{
  "buildCommand": "npm run build && mkdir -p dist/public/attached_assets && cp -R attached_assets/* dist/public/attached_assets/ || true",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "env": {
    "VITE_API_URL": "http://fi4.bot-hosting.net:22135"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://fi4.bot-hosting.net:22135/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "http://fi4.bot-hosting.net:22135/uploads/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "http://fi4.bot-hosting.net:22135/images/:path*"
    },
    {
      "source": "/media/:path*",
      "destination": "http://fi4.bot-hosting.net:22135/media/:path*"
    },
    {
      "source": "/attached_assets/:path*",
      "destination": "http://fi4.bot-hosting.net:22135/media/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/attached_assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, X-Auth-Token"
        }
      ]
    }
  ]
}
